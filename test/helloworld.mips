	begin	1024
	start	1024
	j	MAIN
STR	dw	"Hello World!\n"
MAIN
	addi	$s0, $zero, STR
	lui	$s0, STR		# load the address of the string.
	
	addi	$s1, $zero, MAIN
	lui	$s1, MAIN		# load the address of end of string.
	
LOOP	lw	$t1, 0($s0)
	dout	$t1, 2
	addi	$s0, $s0, 4
	bne	$s0, $s1, LOOP
	
	
	end
