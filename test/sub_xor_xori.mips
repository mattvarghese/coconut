	begin	1000
	start	1024
ARR1	dw	[4]
VAR1	dw	23
VAR2	dw	19

	addi	$s0, $zero, 1024
	addi	$s1, $zero, 256
	addi	$s2, $zero, -128
	addi	$s3, $zero, -32
	
	sub	$t0, $s0, $s1		# 768
	sub	$t1, $s0, $s2		# 1152
	sub	$t2, $s2, $s0		# -1152
	sub	$t3, $s2, $s3		# -96
	
	nop
	nop
	nop
	
	addi	$s0, $zero, 2389
	addi	$s1, $zero, 6880
	addi	$s2, $zero, -2342
	addi	$s3, $zero, -4545
	
	xor	$t0, $s0, $s1		# 5045
	xori	$t0, $s0, 6880		# 5045
	
	xor	$t1, $s0, $s2		# -113
	xori	$t1, $s0, -2342		# -113
	
	xor	$t2, $s2, $s0		# -113
	xori	$t2, $s2, 2389		# -113
	
	xor	$t3, $s2, $s3		# 6373
	xori	$t3, $s2, -4545		# 6373
	
	end
