	begin	1000
	start	1024
ARR1	dw	[4]
VAR1	dw	23
VAR2	dw	19

	addi	$t0, $zero, 12
	addi	$t1, $zero, -23
	addi	$t2, $zero, 24
	addi	$t3, $zero, -46
	
	and	$t4, $t0, $t2	# 12 & 24 = 8
	andi	$t4, $t0, 24
	
	and	$t5, $t1, $t3	# -23 & -46 = -64
	andi	$t5, $t1, -46
	
	and	$t6, $t1, $t0	# 12 & -23 = 8
	andi	$t6, $t1, 12
	andi	$t6, $t0, -23
	
	end
