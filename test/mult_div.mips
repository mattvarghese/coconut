	begin	1000
	start	1024
ARR1	dw	[4]
VAR1	dw	23
VAR2	dw	19

	addi	$t0, $zero, 5
	addi	$t1, $zero, 2
	addi	$t2, $zero, -5
	addi	$t3, $zero, -2
	
	mult	$t0, $t1	# 5,2
	mult	$t0, $t3	# 5,-2
	mult	$t2, $t1	# -5,2
	mult	$t2, $t3	# -5,-2
	
	div	$t0, $t1	# 5,2
	div	$t0, $t3	# 5,-2
	div	$t2, $t1	# -5,2
	div	$t2, $t3	# -5, -2
	
	mfhi	$s0	# -1
	mflo	$s1	# 2
	mthi	$t2	# -5
	mtlo	$t3	# -2
	
	end
