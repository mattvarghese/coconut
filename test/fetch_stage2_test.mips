	begin	1024
	start	1024
	j	MAIN
MEM1	dw	65
MEM2	dw	0
MEM3	dw	2	# output device

MAIN
	addi	$t0, $zero, MEM1
	lui	$t0, MEM1
	
	addi	$t1, $zero, MEM2
	lui	$t1, MEM2
	
	lw	$t2, 0($t0)
	sw	$t2, 0($t1)
	
	lw	$t2, 0($t0)
	dout	$t2, 2
	
	addi	$t3, $zero, 2
	lw	$t2, 0($t1)
	rdout	$t2, $t3
	
	addi	$t4, $zero, MEM3
	lui	$t4, MEM3
	lw	$t3, 0($t4)
	rdout	$t2, $t3
	
	end
